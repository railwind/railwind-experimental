{
  "name": "railwind-experimental",
  "version": "0.2.0",
  "description": "Minimal, Clean and Expandable Components",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && rollup -c",
    "clean": "rimraf -r dist",
    "lint": "eslint 'src/**/*.{ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/railwindlabs/railwind-experimental.git"
  },
  "keywords": [],
  "author": "edwintantawi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/railwindlabs/railwind-experimental/issues"
  },
  "homepage": "https://github.com/railwindlabs/railwind-experimental#readme",
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-loader": "^8.2.5",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "husky": "^8.0.1",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "storybook-addon-themes": "^6.1.0",
    "tailwindcss": "^3.1.8"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18",
    "tailwind-merge": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  }
}
